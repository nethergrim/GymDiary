<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.combogymdiary"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Mybarstyle"
        android:windowSoftInputMode="adjustPan" >
        <activity
            android:name="com.example.combogymdiary.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.combogymdiary.SettingsActivity" >
        </activity>
        <activity android:name="com.example.combogymdiary.StartTrainingActivity" >
        </activity>
        <activity android:name="com.example.combogymdiary.ExersisesList" >
        </activity>
        <activity android:name="com.example.combogymdiary.AddingExersises" >
        </activity>
        <activity android:name="com.example.combogymdiary.AddingProgram" >
        </activity>
        <activity
            android:name="com.example.combogymdiary.TrainingAtProgress"
            android:windowSoftInputMode="stateVisible" >
        </activity>
        <activity
            android:name="com.example.combogymdiary.TrainingListActivity"
            android:label="@string/title_training_list"
            android:parentActivityName="com.example.combogymdiary.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.combogymdiary.MainActivity" />
        </activity>
        <activity
            android:name="com.example.combogymdiary.TrainingDetailActivityty"
            android:label="@string/title_training_detail"
            android:parentActivityName="com.example.combogymdiary.TrainingListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TrainingListActivity" />
        </activity>
        <activity
            android:name="com.example.combogymdiary.TrainingDetailActivity"
            android:label="@string/title_training_detail"
            android:parentActivityName=".TrainingListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TrainingListActivity" />
        </activity>
    </application>

</manifest>
