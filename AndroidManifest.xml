<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nethergrim.combogymdiary"
    android:versionCode="44"
    android:versionName="1.3.7" 
    android:installLocation="auto">

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:backupAgent="com.nethergrim.combogymdiary.TheBackupAgent"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:restoreAnyVersion="true"
        android:theme="@style/Theme.Mybarstyle"
        android:windowSoftInputMode="adjustPan" 
        android:largeHeap="true">
        <meta-data
            android:name="com.google.android.backup.api_key"
            android:value="AEdPqrEAAAAIV4H7afwcMJOgR3ZuDw1eUCyyNdtzLv5evDXyQQ" />
        
        <meta-data android:name="com.google.android.gms.version"
           android:value="40305500"/>

        <activity android:name="com.nethergrim.combogymdiary.MainActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.SettingsActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.MainActivity" />
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.StartTrainingActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.MainActivity" />
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.ExersisesList" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.MainActivity" />
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.AddingExersises"
            android:windowSoftInputMode="stateHidden" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.ExersisesList" />
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.AddingProgram"
            android:windowSoftInputMode="stateHidden" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.StartTrainingActivity" />
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.TrainingAtProgress"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:windowSoftInputMode="stateHidden" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.MainActivity" />
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.HistoryActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:parentActivityName="com.nethergrim.combogymdiary.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.combogymdiary.MainActivity" />
        </activity>
        <activity android:name="com.google.android.gms.ads.AdActivity"
             android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"/>
        <activity android:name="com.nethergrim.combogymdiary.HistoryDetailedActivity"
            android:parentActivityName="com.nethergrim.combogymdiary.HistoryActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.HistoryActivity" />
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.CatalogActivity"
            android:parentActivityName="com.nethergrim.combogymdiary.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.MainActivity" />
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.TempActivity"
            android:label="@string/title_activity_temp" >
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.CatalogDetailedActivity"
            android:parentActivityName="com.nethergrim.combogymdiary.CatalogActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.CatalogActivity" />
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.MeasurementsActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:parentActivityName="com.nethergrim.combogymdiary.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.MainActivity" />
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.AddingMeasurementActivity"
            android:parentActivityName="com.nethergrim.combogymdiary.MeasurementsActivity"
            android:windowSoftInputMode="stateHidden" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.MeasurementsActivity" />
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.MeasurementsDetailedActivity"
            android:parentActivityName="com.nethergrim.combogymdiary.MeasurementsActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.MeasurementsActivity" />
        </activity>
        <activity android:name="com.nethergrim.combogymdiary.EditingProgramAtTraining"
            android:windowSoftInputMode="stateHidden" 
            android:parentActivityName="com.nethergrim.combogymdiary.TrainingAtProgress" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nethergrim.combogymdiary.TrainingAtProgress" />
        </activity>
        <service android:name="MyService" android:process=":myservice"></service>
    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>
