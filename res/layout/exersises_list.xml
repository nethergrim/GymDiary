<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    
    
    android:id="@+id/rlExeList"
    android:layout_width="match_parent"
    android:background="@drawable/cream_pixels_bitmap"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
<com.google.android.gms.ads.AdView
    
    android:id="@+id/adView5"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
   
    />
    
    
    <ListView
        android:id="@+id/listView11"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/btnCreate"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/adView5"
        android:layout_margin="5dp"
        android:background="@drawable/rounded"
        android:focusableInTouchMode="true" >

    </ListView>

    <Button
        android:id="@+id/btnCreate"
        style="@style/HabraThemeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:textColor="@android:color/white"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_margin="5dp"
        android:text="@string/create_new" />

</RelativeLayout>